{
  "build": {
    "beforeBuildCommand": "ng build",
    "beforeDevCommand": "ng serve",
    "devPath": "http://localhost:4200",
    "distDir": "../dist/angular_tauri_app/browser"
  },
  "package": {
    "productName": "my-angulartauri-template",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/vto.png",
        "icons/vto.ico"
      ],
      "identifier": "dcom.tauri.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "my-angulartauri-template",
        "width": 800
      }
    ]
  }
}
